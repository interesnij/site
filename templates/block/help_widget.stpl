<div class="widget">
    <h5 class="widget-title font-alt">Помощь</h5>
    <div class="widget-body">
      <ul class="clearlist widget-menu">
        <% for object in help_cats.iter() { %>
          <li>
            <a href="/helps/<%= object.slug %>/" class="ajax">
              <%= object.name %>
            </a>
          </li>
        <% } %>
      </ul>
    </div> 
</div>
